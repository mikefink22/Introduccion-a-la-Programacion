Algoritmo AyudaLuu
	Definir num Como Entero;
	Definir letra Como Caracter;
	Escribir "Ingrese un número";
	Leer num;
	Si num == 2 Entonces
		Escribir "Hola";
	FinSi
	Segun num Hacer
		2: Escribir "Hola";
		3: Escribir "Chau";
	FinSegun
	Escribir "Ingrese una letra";
	Leer letra;
	Si letra == "a" Entonces
		Escribir "Hola"
	FinSi
	Segun letra hacer
		"a": Escribir "Hola";
	FinSegun
FinAlgoritmo